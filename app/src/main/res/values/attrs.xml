<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="declare_image_text_viewgroup">

        <attr name="image_text_viewgroup_src" format="reference" />
        <attr name="image_text_viewgroup_text" format="string" />

    </declare-styleable>

    <declare-styleable name="AnimatedProgressBar">
        <attr name="progressColor" format="color" />
        <attr name="bidirectionalAnimate" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ProgressWheel">
        <attr name="pwText" format="string" />
        <attr name="pwTextColor" format="color" />
        <attr name="pwTextSize" format="dimension" />
        <attr name="pwBarColor" format="color" />
        <attr name="pwRimColor" format="color" />
        <attr name="pwRimWidth" format="dimension" />
        <attr name="pwSpinSpeed" format="dimension" />
        <attr name="pwDelayMillis" format="integer" />
        <attr name="pwCircleColor" format="color" />
        <attr name="pwRadius" format="dimension" />
        <attr name="pwBarWidth" format="dimension" />
        <attr name="pwBarLength" format="dimension" />
        <attr name="pwContourColor" format="color" />
        <attr name="pwContourSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DilatingDotsProgressBar">
        <attr name="android:radius" />
        <attr name="android:color" />
        <attr name="dd_numDots" format="integer" />
        <attr name="dd_scaleMultiplier" format="float" />
        <attr name="dd_animationDuration" format="integer" />
        <attr name="dd_horizontalSpacing" format="dimension" />
        <attr name="dd_endColor" format="color"/>
    </declare-styleable>

</resources>